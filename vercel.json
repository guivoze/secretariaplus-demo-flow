{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "installCommand": "npm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/api/facebook-conversion",
      "destination": "/api/facebook-conversion.js"
    }
  ],
  "functions": {
    "api/facebook-conversion.js": {
      "maxDuration": 10
    }
  },
  "env": {
    "NODE_VERSION": "18"
  }
}